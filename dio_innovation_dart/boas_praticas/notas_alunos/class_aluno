class Aluno{
    String _nome='';
    List<double> _notas =[];


    Aluno(String nome){

        this._nome = nome;

    }

    void setNome(String nome){
        _nome = nome;

    }

    String getNome(){
        return _nome;
    }

    void addNotas(double notas){
        _notas.add(_notas);
    }
}